<?xml version="1.0" encoding="UTF-8"?>
<iso_project image_name="rom-flasher-cd.bin" cue_sheet="rom-flasher-cd.cue" no_xa="0">
	<track type="data">
		<identifiers
			system          ="PLAYSTATION"
			application     ="PLAYSTATION"
			volume          ="XFLASH"
			volume_set      ="XFLASH"
			publisher       ="ALEXFREE"
			data_preparer   ="MKPSXISO"
			copyright       ="3-BSD"
		/>

		<!--
			Use the Japanese license file.

			This is important since Japanese consoles check the game's region.
		-->
		<license file="licensej.dat" />

		<directory_tree>
			<!-- System configuration file -->
			<file name="SYSTEM.CNF" type="data" source="system.cnf" />

			<!-- Main executable -->
			<file name="NXFLASH.EXE" type="data" source="NXFLASH.EXE" />

			<!-- ROM DAT file -->
			<file name="ROMFILE.DAT" type="data" source="romfile.dat" />

			<!-- Generate a 16MB (8192 sectors) dummy file -->
			<dummy sectors="8192" />
		</directory_tree>
	</track>
</iso_project>
